{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react-jsx",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
     "moduleResolution": "bundler",
     "moduleDetection": "force",
     "sourceMap": true
  },
  "include": ["src/**/*"]
}